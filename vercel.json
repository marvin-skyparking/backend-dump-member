{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/node",
        "config": {
          "distDir": "dist"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/dist/server.js"
      }
    ],
    "env": {
      "NODE_ENV": "production"
    },
    "functions": {
      "api/**/*.js": {
        "maxDuration": 10
      }
    }
  }
  