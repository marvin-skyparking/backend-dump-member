{
  "name": "backend-dump-member",
  "version": "1.0.0",
  "main": "dist/server.js",
  "license": "MIT",
  "scripts": {
    "test": "npx jest --passWithNoTests",
    "lint": "npx eslint . --fix",
    "lint:build": "npx eslint . --fix && yarn build",
    "prepare": "husky",
    "build": "tsc && yarn copy-static-files",
    "start": "yarn build && node dist/server.js",
    "dev": "yarn prettier:format && ts-node-dev --respawn --transpile-only src/server.ts",
    "migration:generate": "sequelize-cli migration:generate",
    "migrate": "sequelize-cli db:migrate",
    "migrate:prod": "yarn ts-node src/migrate.ts",
    "migration:undo": "sequelize-cli db:migrate:undo",
    "migration:undo:all": "sequelize-cli db:migrate:undo:all",
    "prettier:format": "prettier --config .prettierrc src/**/*.ts --write",
    "seed": "sequelize-cli db:seed:all",
    "seed:undo": "sequelize-cli db:seed:undo",
    "copy-static-files": "yarn copy-files && yarn copy-directories",
    "copy-files": "copyfiles -u 1 './src/uploads/**/*.*' './dist/uploads/'",
    "copy-directories": "copyfiles -u 1 './src/uploads/**/' './dist/uploads/'"
},
  "lint-staged": {
    "*.ts": "eslint --fix"
  },
  "dependencies": {
    "@sentry/node": "^8.26.0",
    "@types/node": "^22.3.0",
    "@types/sequelize": "^4.28.20",
    "axios": "^1.7.5",
    "body-parser": "^1.20.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "date-fns": "^3.6.0",
    "dotenv": "^16.4.5",
    "exceljs": "^4.4.0",
    "express": "^4.19.2",
    "express-rate-limit": "^7.4.0",
    "http-status-codes": "^2.3.0",
    "mariadb": "^3.3.1",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.11.0",
    "reflect-metadata": "^0.2.2",
    "sequelize": "^6.37.3",
    "sequelize-typescript": "^2.1.6",
    "serverless-http": "^3.2.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.5",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/crypto-js": "^4.2.2",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/mocha": "^10.0.7",
    "@types/multer": "^1.4.11",
    "@types/supertest": "^6.0.2",
    "@types/swagger-ui-express": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "^8.2.0",
    "@typescript-eslint/parser": "^8.2.0",
    "copyfiles": "^2.4.1",
    "eslint": "^9.9.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.35.0",
    "globals": "^15.9.0",
    "husky": "^9.1.4",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.9",
    "nodemon": "^3.1.4",
    "prettier": "^3.3.3",
    "sequelize-cli": "^6.6.2",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.4",
    "ts-node-dev": "^2.0.0",
    "vercel": "^36.0.0"
  }
}
