{
    "builds": [
      {
        "src": "src/app.ts",
        "use": "@vercel/node",
        "config": {
          "includeFiles": "dist/**"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "dist/server.js"
      }
    ],
    "build": {
      "env": {
        "BUILD_COMMAND": "yarn build"
      }
    }
  }
  